{
  "name": "yearbook_production-client",
  "version": "1.0.0",
  "description": "Web GUI for yearbook_production",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vase444ka/yearbook_production",
    "directory": "client"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build:client": "webpack",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "yarn build:server && yarn build:client",
    "watch:client": "webpack --watch",
    "serve": "node ./dist/index.js",
    "watch": "yarn prebuild && yarn build:server && ttab -t 'Running server' yarn serve && ttab -t 'Watching client' yarn watch:client",
    "start": "yarn watch && sleep 5 && open http://localhost:3000",
    "test": "echo 'sam ty yebook' && exit 0"
  },
  "author": {
